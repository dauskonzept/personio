                            </label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <itemsProcFunc>DSKZPT\Personio\Hook\ItemsProcFunc->getFilterValues</itemsProcFunc>
                            </config>
                        </TCEforms>
                    </settings.filter.occupation>
                    <!-- OccupationCategory -->
                    <settings.filter.occupationCategory>
                        <TCEforms>
                            <label>
                                LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_filter.occupationCategory
                            </label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
