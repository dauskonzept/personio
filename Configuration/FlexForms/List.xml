                                    </numIndex>
                                    <numIndex index="1">pt</numIndex>
                                </numIndex>
                            </items>
                        </config>
                    </settings.overwrite.language>

                    <!-- Show Page Uid -->
                    <settings.overwrite.showPageUid>
                        <label>
                            LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.showPageUid
                        </label>
                        <config>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                            <minitems>0</minitems>
                        </config>
                    </settings.overwrite.showPageUid>
                </el>
            </ROOT>
