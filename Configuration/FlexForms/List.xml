<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>
                        LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_list_tab.settings
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <!-- Feed URL -->
                    <settings.overwrite.feedUrl>
                        <TCEforms>
                            <label>
                                LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.feedUrl
                            </label>
                            <config>
                                <checkbox>1</checkbox>
                                <default></default>
                                <type>input</type>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </settings.overwrite.feedUrl>
                    <!-- Language -->
                    <settings.overwrite.language>
                        <TCEforms>
                            <label>
                                LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.language
                            </label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.language.0
                                        </numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">
                                            LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.language.1
                                        </numIndex>
                                        <numIndex index="1">de</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">
                                            LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.language.2
                                        </numIndex>
                                        <numIndex index="1">en</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">
                                            LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.language.3
                                        </numIndex>
                                        <numIndex index="1">fr</numIndex>
                                    </numIndex>
                                    <numIndex index="4">
                                        <numIndex index="0">
                                            LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.language.4
                                        </numIndex>
                                        <numIndex index="1">es</numIndex>
                                    </numIndex>
                                    <numIndex index="5">
                                        <numIndex index="0">
                                            LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.language.5
                                        </numIndex>
                                        <numIndex index="1">nl</numIndex>
                                    </numIndex>
                                    <numIndex index="6">
                                        <numIndex index="0">
                                            LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.language.6
                                        </numIndex>
                                        <numIndex index="1">it</numIndex>
                                    </numIndex>
                                    <numIndex index="7">
                                        <numIndex index="0">
                                            LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.language.7
                                        </numIndex>
                                        <numIndex index="1">pt</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.overwrite.language>
                    <!-- Show Page Uid -->
                    <settings.overwrite.showPageUid>
                        <TCEforms>
                            <label>
                                LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.showPageUid
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.overwrite.showPageUid>
                    <!-- Custom Text -->
                    <settings.text>
                        <TCEforms>
                            <label>
                                LLL:EXT:personio/Resources/Private/Language/locallang_be.xlf:flexforms_settings.text
                            </label>
                            <config>
                                <type>input</type>
                                <size>255</size>
                                <max>255</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.text>
                </el>
            </ROOT>
        </sDEF>
        <filter>
