                                <itemsProcFunc>DSKZPT\Personio\Hook\ItemsProcFunc->getFilterValues</itemsProcFunc>
                            </config>
                        </TCEforms>
                    </settings.filter.schedule>
                    <!-- Occupation -->
                    <settings.filter.occupation>
                        <TCEforms>
                            <label>
